[?2004h]0;bmscecse@bmscecse-HP-Elite-Tower-600-G9-Desktop-PC: ~/LAB3[01;32mbmscecse@bmscecse-HP-Elite-Tower-600-G9-Desktop-PC[00m:[01;34m~/LAB3[00m$ cat leapyear
[?2004l#!bin/bash/
#check if the year is leap or not
echo "enter the year: "
read year

if [ `expr $year % 4` -eq 0 ] 
then 
	echo "it is a leap year"
else 
	echo "it is not a leap year"
fi
